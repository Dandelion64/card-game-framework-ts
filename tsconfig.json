{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution":"node",
    "baseUrl": ".",
    "rootDir": "src",
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "skipLibCheck": true
  }
}
